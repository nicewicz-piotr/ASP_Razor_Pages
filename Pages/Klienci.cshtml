@page
@model NorthwindWeb.Pages.KlienciModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<div class="row">
    <h1 class="display-2">Klienci</h1>
    <table class="table">
        <thead class="thead-inverse">
            <tr><th>Stanowisko</th></tr>
        </thead>
        <tbody>
            @foreach (var item in Model.Klienci)
            {
                <tr><td><a href="/klient?id=@item.CustomersIDs">@item.CustomersTites</a></td></tr>
            }
        </tbody>
    </table>
</div>